class Solution {
public:
    bool isPalindrome(int x) {
        string s = to_string(x);
        int j = s.length() - 1;
        if (x < 0)
            return false;
        if (x > -1 && x < 10)
            return true;
        for (int i = 0; i < j && i != j ;i++) {
            if (s[i] != s[j])
                return false;
            j--;
        }
        return true;
    }
};
